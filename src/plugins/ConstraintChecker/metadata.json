{
  "id": "ConstraintChecker",
  "name": "Constraint Checker",
  "version": "1.0.0",
  "description": "Made to run from webhook.",
  "icon": {
    "class": "glyphicon glyphicon-cog",
    "src": ""
  },
  "disableServerSideExecution": true,
  "disableBrowserSideExecution": true,
  "writeAccessRequired": false,
  "configStructure": [
    {
      "name": "constraintType",
      "displayName": "Constraint type",
      "description": "Which constraints to check",
      "value": "META",
      "valueType": "string",
      "valueItems": [
        "META",
        "CUSTOM",
        "BOTH"
      ]
    },
    {
      "name": "mongoUri",
      "displayName": "mongoUri",
      "description": "mongoUri",
      "value": "mongodb://127.0.0.1:27017/webgme_constraint_results",
      "valueType": "string"
    }
  ]
}